-container {\r\n    position: fixed;\r\n    top: 0;\r\n    left: 0;\r\n    width: 100vw;\r\n    height: 100vh;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    background: linear-gradient(135deg, rgba(2, 0, 0.6, 1) 0%, rgba(9, 9, 121, 1) 35%, rgba(0, 212, 255, 1) 100%);\r\n    overflow: hidden;\r\n}\r\n\r\n/* Animated background boxes */\r\n.animated-boxes {\r\n    position: absolute;\r\n    width: 100%;\r\n    height: 100%;\r\n    overflow: hidden;\r\n}\r\n\r\n.box {\r\n    position: absolute;\r\n    width: 40px;\r\n    height: 40px;\r\n    background-color: transparent;\r\n    border: 4px solid rgba(255, 255, 255, 0.6);\r\n    animation: animate 10s linear infinite;\r\n    opacity: 0;\r\n}\r\n\r\n/* Box positions */\r\n.box-1 { top: 3%; left: 25%; animation-duration: 10s; }\r\n.box-2 { top: 40%; left: 6%; animation-duration: 7s; }\r\n.box-3 { top: 5%; left: 60%; animation-duration: 9s; }\r\n.box-4 { top: 27%; left: 90%; animation-duration: 10s; }\r\n.box-5 { top: 67%; left: 10%; animation-duration: 6s; }\r\n.box-6 { top: 88%; left: 70%; animation-duration: 6s; }\r\n.box-7 { top: 60%; left: 80%; animation-duration: 15s; }\r\n.box-8 { top: 4%; left: 4%; animation-duration: 12s; }\r\n.box-9 { top: 90%; left: 25%; animation-duration: 9s; }\r\n.box-10 { top: 20%; left: 80%; animation-duration: 5s; }\r\n\r\n@keyframes animate {\r\n    0% {\r\n        transform: scale(0) translateY(0) rotate(0);\r\n        opacity: 1;\r\n    }\r\n    100% {\r\n        transform: scale(1.3) translateY(-90px) rotate(360deg);\r\n        opacity: 0;\r\n    }\r\n}\r\n\r\n/* Login form styling */\r\n.login-form {\r\n    width: 90%;\r\n    max-width: 450px;\r\n    padding: 2.5rem;\r\n    background-color: rgba(255, 255, 255, 0.9);\r\n    border-radius: 12px;\r\n    box-shadow: 0 10px 25px rgba(0, 0, 0, 0.2);\r\n    z-index: 10;\r\n    margin: 20px; /* Small margin on very small screens */\r\n}\r\n\r\n.logo-container {\r\n    text-align: center;\r\n    margin-bottom: 1.5rem;\r\n}\r\n\r\n.logo {\r\n    width: 120px;\r\n    height: auto;\r\n    max-width: 100%;\r\n}\r\n\r\n.welcome-text {\r\n    color: #0f5f96;\r\n    text-align: center;\r\n    margin-bottom: 2rem;\r\n    font-size: clamp(1.5rem, 5vw, 1.8rem);\r\n}\r\n\r\n.form-group {\r\n    margin-bottom: 1.5rem;\r\n}\r\n\r\nlabel {\r\n    display: block;\r\n    margin-bottom: 0.5rem;\r\n    color: #0f5f96;\r\n    font-weight: 600;\r\n    font-size: clamp(0.9rem, 3vw, 1rem);\r\n}\r\n\r\n.form-input {\r\n    width: 100%;\r\n    padding: 0.8rem 1rem;\r\n    border: 1px solid #ddd;\r\n    border-radius: 8px;\r\n    font-size: 1rem;\r\n    transition: all 0.3s ease;\r\n}\r\n\r\n.form-input:focus {\r\n    outline: none;\r\n    border-color: #2199ea;\r\n    box-shadow: 0 0 0 2px rgba(33, 153, 234, 0.2);\r\n}\r\n\r\n.error-message {\r\n    color: #dc3545;\r\n    margin: 1rem 0;\r\n    text-align: center;\r\n    font-weight: 500;\r\n    font-size: clamp(0.8rem, 3vw, 0.9rem);\r\n}\r\n\r\n.login-button {\r\n    width: 100%;\r\n    padding: 0.9rem;\r\n    background-color: #0f5f96;\r\n    color: white;\r\n    border: none;\r\n    border-radius: 8px;\r\n    font-size: 1rem;\r\n    font-weight: 600;\r\n    cursor: pointer;\r\n    transition: all 0.3s ease;\r\n    margin-top: 0.5rem;\r\n}\r\n\r\n.login-button:hover {\r\n    background-color: #2199ea;\r\n    transform: translateY(-2px);\r\n}\r\n\r\n/* Responsive adjustments */\r\n@media (max-width: 480px) {\r\n    .login-form {\r\n        padding: 1.5rem;\r\n        width: 95%;\r\n    }\r\n    \r\n    .logo {\r\n        width: 100px;\r\n    }\r\n    \r\n    .box {\r\n        width: 30px;\r\n        height: 30px;\r\n        border-width: 3px;\r\n    }\r\n}\r\n\r\n@media (max-height: 600px) {\r\n    .login-form {\r\n        padding: 1.5rem;\r\n        margin-top: 20px;\r\n        margin-bottom: 20px;\r\n        overflow-y: auto;\r\n        max-height: 90vh;\r\n    }\r\n}\r\n</style>\r\n"],"mappings":";OAQqBA,UAAsC;;EAPlDC,KAAK,EAAC;A